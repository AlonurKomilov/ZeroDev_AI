# .env.example - rename to .env and fill in your values for local development

# Set to "production" to enable production features
ENVIRONMENT=development

# Set to "True" for development features like debug mode
DEBUG=False

# PostgreSQL Configuration
POSTGRES_SERVER=localhost
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=zerodev

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# OpenAI API Key
OPENAI_API_KEY="your_openai_api_key_here"

# Vault Configuration
VAULT_ADDR="http://127.0.0.1:8200"
VAULT_TOKEN="your_vault_token_here"
